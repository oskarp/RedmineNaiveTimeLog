        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RedmineTimeTask"
        mc:Ignorable="d"
        Title="about" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="104,238,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <RichTextBox x:Name="aboutBox" HorizontalAlignment="Left" Height="223" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" IsDocumentEnabled="True" IsReadOnly="True" BorderThickness="0">
        </RichTextBox>

    </Grid>
